/* installation directory is "BINDIR = /usr/X11R6/bin" by default */

XPMLIB = -L/usr/X11R6/lib -lXpm -lm  
DEPLIBS = $(DEPXLIB) 

LOCAL_LIBRARIES = $(XPMLIB) $(XLIB)  

LINTLIBS = $(LINTXLIB)

EXTRA_DEFINES = -Debug         /* CFLAGS = -Debug */

SRCS = wmload.c
OBJS = wmload.o

ComplexProgramTarget(wmload)
